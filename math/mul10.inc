.include	"./math/mul8x8.inc"

;--------------------------------------------------------
MUL10:
;--------------------------------------------------------
;Умножение 8b числа на 10
;IN: TEMP-8b число
;OUT: TEMP_H/L-результат
;--------------------------------------------------------
	LDI TEMP_L,0x0a
	MCALL MUL8X8
	RET
.endif
